{
    "name": "pandawan-technology/neo4j-fixtures-bundle",
    "description": "Allow to load Neo4j data fixtures into your Symfony application",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "graph",
        "database",
        "neo4j",
        "Symfony",
        "fixtures",
        "data"
    ],
    "homepage": "https://github.com/PandawanTechnology/Neo4jFixturesBundle",
    "authors": [
        {
            "name": "Xavier Coureau",
            "email": "xavier@pandawan-technology.com"
        }
    ],
    "require": {
        "pandawan-technology/neo4j-bundle": "dev-master",
        "pandawan-technology/neo4j-data-fixtures": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PandawanTechnology\\Neo4jFixturesBundle\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    }
}
